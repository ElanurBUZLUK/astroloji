# Application
DEBUG=false
LOG_LEVEL=INFO
ALLOWED_ORIGINS=http://localhost:3000

# Database & Cache
POSTGRES_URL=postgresql://astro_user:astro_secure_pass_2025@localhost:5432/astro_aa
REDIS_URL=redis://localhost:6379/0

# RAG / Vector Store
SEARCH_BACKEND=QDRANT
BM25_LANGUAGE=turkish
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
QDRANT_COLLECTION=astro_knowledge
OPENSEARCH_URL=http://localhost:9200
OPENSEARCH_USER=admin
OPENSEARCH_PASSWORD=admin
OPENSEARCH_INDEX=astro_docs
HYBRID_ALPHA=0.6

# LLM Provider
OPENAI_API_KEY=
OPENAI_BASE_URL=
LLM_MODEL=gpt-4o-mini

# Embeddings / Reranker
USE_EMBEDDING_MODEL=1
EMBEDDING_MODEL=sentence-transformers/paraphrase-multilingual-mpnet-base-v2
RERANKER_MODEL=BAAI/bge-reranker-large

# Ephemeris
SWISSEPH_DATA_PATH=backend/swisseph_data

# Feature Flags
ENABLE_METRICS_ENDPOINT=true
ENABLE_ADMIN_ENDPOINTS=false
